{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@types/react-redux": "^7.1.19",
    "axios": "^0.22.0",
    "node-sass": "^6.0.1",
    "pubsub-js": "^1.9.3",
    "react": "17.x",
    "react-dom": "17.x",
    "react-redux": "^7.2.5",
    "umi": "^3.5.20",
    "uuidjs": "^4.2.9"
  },
  "devDependencies": {
    "@types/pubsub-js": "^1.8.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
